{
  "name": "autobot",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "dev:bot": "pnpm --filter @autobot/bot dev",
    "dev:dashboard": "pnpm --filter @autobot/dashboard dev",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "typecheck": "pnpm -r typecheck",
    "db:generate": "pnpm --filter @autobot/db generate",
    "db:migrate": "pnpm --filter @autobot/db migrate",
    "db:push": "pnpm --filter @autobot/db push",
    "db:studio": "pnpm --filter @autobot/db studio",
    "test": "pnpm -r test",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint-config-prettier": "^9.1.0",
    "tsx": "^4.7.0",
    "vitest": "^1.1.0"
  },
  "pnpm": {
    "overrides": {}
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
